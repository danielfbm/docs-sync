{
    "compilerOptions": {
      "jsx": "react-jsx",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "target": "ESNext",
    },
    "mdx": {
      "checkMdx": true,
    },
  }
  